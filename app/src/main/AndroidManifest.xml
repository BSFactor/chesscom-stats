<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/chess_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/chess_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.StatsForChesscom">
        <activity
            android:name="MainActivity"
            android:exported="true"
            android:theme="@style/Theme.StatsForChesscom">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--        <activity-->
        <!--            android:name=".ui.playerdetail.PlayerDetailActivity"-->
        <!--            android:exported="false" />-->
    </application>
    <attribution
        android:label="@string/audio_playback_attribution_label"
        android:tag="audioPlayback" />
</manifest>